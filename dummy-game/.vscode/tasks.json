{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "dummy-game: run"
		},
		{
			"type": "process",
			"command":"wasm-pack",
			"args": ["build", "--target", "web"],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "dummy-game: wasm-pack build"
		}
	]
}